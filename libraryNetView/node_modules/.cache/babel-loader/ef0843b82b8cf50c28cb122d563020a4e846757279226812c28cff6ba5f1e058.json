{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport { ref, reactive, onMounted, watch, getCurrentInstance } from 'vue';\nimport request from \"@/utils/request.js\";\nimport { clearToken } from \"@/utils/storage\";\nimport AdminMenu from '@/components/VerticalMenu.vue';\nimport Logo from '@/components/Logo.vue';\nimport LevelHeader from '@/components/LevelHeader.vue';\nimport { UserFilled, Plus, User, Message } from '@element-plus/icons-vue';\nexport default {\n  name: \"AdminHome\",\n  components: {\n    Logo,\n    LevelHeader,\n    AdminMenu,\n    UserFilled,\n    Plus,\n    User,\n    Message\n  },\n  setup() {\n    const {\n      proxy\n    } = getCurrentInstance();\n\n    // 响应式数据\n    const adminRoutes = ref([]);\n    const userInfo = reactive({\n      id: null,\n      url: '',\n      name: '',\n      role: null,\n      email: ''\n    });\n    const flag = ref(false);\n    const tag = ref('仪表盘');\n    const colorLogo = ref('#333');\n    const bagMenu = ref('transparent');\n    const dialogOperaion = ref(false);\n\n    // 方法定义\n    // 更新页面标题\n    const updatePageTitle = () => {\n      const currentPath = proxy.$route.path;\n      const currentRoute = adminRoutes.value.find(route => route.path === currentPath);\n      if (currentRoute) {\n        tag.value = currentRoute.name;\n      }\n    };\n\n    // 加载侧边栏状态\n    const loadSidebarState = () => {\n      flag.value = sessionStorage.getItem('flag') === 'true';\n    };\n\n    // 处理头像上传成功\n    const handleAvatarSuccess = res => {\n      if (res.code !== 200) {\n        proxy.$message.error('头像上传失败');\n        return;\n      }\n      proxy.$message.success('头像上传成功');\n      userInfo.url = res.data;\n    };\n\n    // 更新用户信息\n    const updateUserInfo = async () => {\n      try {\n        const userUpdateDTO = {\n          userAvatar: userInfo.url,\n          userName: userInfo.name,\n          userEmail: userInfo.email\n        };\n        const response = await proxy.$axios.put('/user/update', userUpdateDTO);\n        const {\n          data\n        } = response;\n        if (data.code === 200) {\n          dialogOperaion.value = false;\n          tokenCheckLoad();\n          proxy.$swal.fire({\n            title: '修改成功',\n            text: data.msg,\n            icon: 'success',\n            showConfirmButton: false,\n            timer: 1000\n          });\n        }\n      } catch (error) {\n        dialogOperaion.value = false;\n        proxy.$swal.fire({\n          title: '修改失败',\n          text: error.message || '发生未知错误',\n          icon: 'error',\n          showConfirmButton: false,\n          timer: 2000\n        });\n        console.error('修改个人信息异常:', error);\n      }\n    };\n\n    // 处理头部事件\n    const eventListener = event => {\n      if (event === 'center') {\n        dialogOperaion.value = true;\n      } else if (event === 'loginOut') {\n        loginOut();\n      }\n    };\n\n    // 退出登录\n    const loginOut = async () => {\n      const confirmed = await proxy.$swalConfirm({\n        title: '确认退出登录?',\n        text: '退出后需重新登录',\n        icon: 'warning',\n        showCancelButton: true,\n        confirmButtonText: '确认',\n        cancelButtonText: '取消',\n        confirmButtonColor: '#ff5722',\n        cancelButtonColor: '#909399',\n        iconColor: '#ff9800',\n        customClass: {\n          popup: 'modern-swal-popup',\n          header: 'modern-swal-header',\n          title: 'modern-swal-title',\n          content: 'modern-swal-content',\n          confirmButton: 'modern-swal-confirm-button',\n          cancelButton: 'modern-swal-cancel-button',\n          actions: 'modern-swal-actions'\n        },\n        buttonsStyling: true,\n        showClass: {\n          popup: 'animate__animated animate__fadeInUp animate__faster'\n        },\n        hideClass: {\n          popup: 'animate__animated animate__fadeOutDown animate__faster'\n        }\n      });\n      if (confirmed) {\n        proxy.$swal.fire({\n          title: '退出成功',\n          text: '正在返回登录页面...',\n          icon: 'success',\n          showConfirmButton: false,\n          timer: 1000,\n          iconColor: '#67C23A',\n          customClass: {\n            popup: 'modern-swal-popup',\n            title: 'modern-swal-title',\n            content: 'modern-swal-content'\n          }\n        });\n        setTimeout(() => {\n          clearToken();\n          proxy.$router.push(\"/login\");\n        }, 1000);\n      }\n    };\n\n    // 切换侧边栏\n    const selectOperation = flagValue => {\n      flag.value = flagValue;\n      sessionStorage.setItem('flag', flagValue);\n    };\n\n    // 选择菜单项\n    这里的path;\n    const handleRouteSelect = path => {\n      if (proxy.$route.path === path) return;\n      const route = adminRoutes.value.find(item => item.path === path);\n      if (route) {\n        tag.value = route.name;\n        proxy.$router.push(path);\n      }\n    };\n\n    // 验证token并加载用户信息\n    const tokenCheckLoad = async () => {\n      try {\n        const res = await request.get('user/auth');\n\n        // 处理错误\n        if (res.data.code === 400) {\n          proxy.$message.error(res.data.msg);\n          proxy.$router.push('/login');\n          return;\n        }\n\n        // 设置用户信息\n        const {\n          id,\n          userAvatar: url,\n          userName: name,\n          userRole: role,\n          userEmail: email\n        } = res.data.data;\n        Object.assign(userInfo, {\n          id,\n          url,\n          name,\n          role,\n          email\n        });\n\n        // 验证用户角色\n        if (role !== 1) {\n          proxy.$message.warning('您没有管理员权限，正在跳转...');\n          proxy.$router.push('/user');\n        }\n      } catch (error) {\n        console.error('获取用户信息失败:', error);\n        proxy.$message.error('认证失败，请重新登录');\n        proxy.$router.push('/login');\n      }\n    };\n\n    // 路由监听\n    watch(() => proxy.$route, () => {\n      updatePageTitle();\n    });\n\n    // 生命周期\n    onMounted(() => {\n      // 直接使用路由配置 - 避免 Vue Router 4 的复杂性\n      const adminRoutes_static = [{\n        path: \"/adminLayout\",\n        name: '数据总览',\n        icon: 'DataAnalysis'\n      }, {\n        path: \"/userManage\",\n        name: '用户管理',\n        icon: 'User'\n      }, {\n        path: \"/bookManage\",\n        name: '书籍管理',\n        icon: 'Reading'\n      }, {\n        path: \"/bookOrderHistoryManage\",\n        name: '借阅记录',\n        icon: 'View'\n      }, {\n        path: \"/bookRssHistoryManage\",\n        name: '图书订阅',\n        icon: 'Connection'\n      }, {\n        path: \"/rssNotificationManage\",\n        name: '订阅通知',\n        icon: 'Message'\n      }, {\n        path: \"/bookshelfManage\",\n        name: '书架管理',\n        icon: 'Folder'\n      }, {\n        path: \"/categoryManage\",\n        name: '类别管理',\n        icon: 'CollectionTag'\n      }, {\n        path: \"/readerProposalManage\",\n        name: '读者建议',\n        icon: 'EditPen'\n      }, {\n        path: \"/noticeManage\",\n        name: '公告管理',\n        icon: 'Bell'\n      }, {\n        path: \"/userOperationLogManage\",\n        name: '行为日志',\n        icon: 'Notebook'\n      }];\n      adminRoutes.value = adminRoutes_static;\n      console.log('设置admin路由:', adminRoutes.value);\n\n      // 初始化数据\n      tokenCheckLoad();\n      loadSidebarState();\n\n      // 初始化标题\n      updatePageTitle();\n    });\n    return {\n      adminRoutes,\n      userInfo,\n      flag,\n      tag,\n      colorLogo,\n      bagMenu,\n      dialogOperaion,\n      updatePageTitle,\n      loadSidebarState,\n      handleAvatarSuccess,\n      updateUserInfo,\n      eventListener,\n      loginOut,\n      selectOperation,\n      handleRouteSelect,\n      tokenCheckLoad\n    };\n  }\n};","map":{"version":3,"names":["ref","reactive","onMounted","watch","getCurrentInstance","request","clearToken","AdminMenu","Logo","LevelHeader","UserFilled","Plus","User","Message","name","components","setup","proxy","adminRoutes","userInfo","id","url","role","email","flag","tag","colorLogo","bagMenu","dialogOperaion","updatePageTitle","currentPath","$route","path","currentRoute","value","find","route","loadSidebarState","sessionStorage","getItem","handleAvatarSuccess","res","code","$message","error","success","data","updateUserInfo","userUpdateDTO","userAvatar","userName","userEmail","response","$axios","put","tokenCheckLoad","$swal","fire","title","text","msg","icon","showConfirmButton","timer","message","console","eventListener","event","loginOut","confirmed","$swalConfirm","showCancelButton","confirmButtonText","cancelButtonText","confirmButtonColor","cancelButtonColor","iconColor","customClass","popup","header","content","confirmButton","cancelButton","actions","buttonsStyling","showClass","hideClass","setTimeout","$router","push","selectOperation","flagValue","setItem","这里的path","handleRouteSelect","item","get","userRole","Object","assign","warning","adminRoutes_static","log"],"sources":["E:\\code\\IntelliJ_IDEAProjects\\LibrarySystem\\libraryNetView\\src\\views\\admin\\Home.vue"],"sourcesContent":["<template>\n    <div class=\"admin-layout\">\n        <!-- 左侧导航 -->\n        <div class=\"sidebar\" :class=\"{'sidebar-collapsed': flag}\">\n            <div class=\"logo-container\">\n                <Logo name=\"书海智控台\" :flag=\"flag\" :bag=\"colorLogo\" />\n            </div>\n            <div class=\"menu-wrapper\">\n                <!-- @select监听子组件handleSelect方法中emit('select', activeIndex.value)传递过来的参数 -->\n                <AdminMenu :flag=\"flag\" :routes=\"adminRoutes\" :bag=\"bagMenu\" @select=\"handleRouteSelect\" />\n            </div>\n        </div>\n\n        <!-- 右侧内容区 -->\n        <div class=\"main-container\" :class=\"{'main-expanded': flag}\">\n            <div class=\"header\">\n                <LevelHeader \n                    @eventListener=\"eventListener\" \n                    @selectOperation=\"selectOperation\" \n                    :tag=\"tag\"\n                    :userInfo=\"userInfo\" \n                />\n            </div>\n            <div class=\"content\">\n                <router-view></router-view>\n            </div>\n        </div>\n\n        <!-- 个人中心弹窗 -->\n        <el-dialog \n            v-model=\"dialogOperaion\" \n            width=\"380px\" \n            custom-class=\"user-center-dialog\"\n            :show-close=\"true\">\n            <div class=\"dialog-content-wrapper\">\n            <div class=\"user-center-header\">\n                <h3>个人中心</h3>\n            </div>\n            <div class=\"user-center-body\">\n                <div class=\"form-item\">\n                    <label class=\"form-label\">\n                        <el-icon><UserFilled /></el-icon>\n                        <span>头像</span>\n                    </label>\n                    <div class=\"avatar-wrapper\">\n                        <el-upload \n                            class=\"avatar-uploader\" \n                            action=\"/api/library-system-api/v1.0/file/upload\"\n                            :show-file-list=\"false\" \n                            :on-success=\"handleAvatarSuccess\">\n                            <img v-if=\"userInfo.url\" :src=\"userInfo.url\" class=\"user-avatar\">\n                            <el-icon v-else class=\"avatar-uploader-icon\"><Plus /></el-icon>\n                        </el-upload>\n                    </div>\n                </div>\n                <div class=\"form-item\">\n                    <label class=\"form-label\">\n                        <el-icon><User /></el-icon>\n                        <span>用户名</span>\n                    </label>\n                    <el-input v-model=\"userInfo.name\" placeholder=\"请输入用户名\"></el-input>\n                </div>\n                <div class=\"form-item\">\n                    <label class=\"form-label\">\n                        <el-icon><Message /></el-icon>\n                        <span>邮箱</span>\n                    </label>\n                    <el-input v-model=\"userInfo.email\" placeholder=\"请输入邮箱\"></el-input>\n                </div>\n            </div>\n                <div class=\"user-center-footer\">\n                <el-button @click=\"dialogOperaion = false\">取 消</el-button>\n                <el-button type=\"primary\" @click=\"updateUserInfo\">确认修改</el-button>\n                </div>\n            </div>\n        </el-dialog>\n    </div>\n</template>\n\n<script>\nimport { ref, reactive, onMounted, watch, getCurrentInstance } from 'vue'\nimport request from \"@/utils/request.js\"\nimport { clearToken } from \"@/utils/storage\"\nimport AdminMenu from '@/components/VerticalMenu.vue'\nimport Logo from '@/components/Logo.vue'\nimport LevelHeader from '@/components/LevelHeader.vue'\nimport { UserFilled, Plus, User, Message } from '@element-plus/icons-vue'\n\nexport default {\n    name: \"AdminHome\",\n    components: {\n        Logo,\n        LevelHeader,\n        AdminMenu,\n        UserFilled,\n        Plus,\n        User,\n        Message\n    },\n    setup() {\n        const { proxy } = getCurrentInstance()\n        \n        // 响应式数据\n        const adminRoutes = ref([])\n        const userInfo = reactive({\n            id: null,\n            url: '',\n            name: '',\n            role: null,\n            email: ''\n        })\n        const flag = ref(false)\n        const tag = ref('仪表盘')\n        const colorLogo = ref('#333')\n        const bagMenu = ref('transparent')\n        const dialogOperaion = ref(false)\n\n        // 方法定义\n        // 更新页面标题\n        const updatePageTitle = () => {\n            const currentPath = proxy.$route.path\n            const currentRoute = adminRoutes.value.find(route => route.path === currentPath)\n            if (currentRoute) {\n                tag.value = currentRoute.name\n            }\n        }\n        \n        // 加载侧边栏状态\n        const loadSidebarState = () => {\n            flag.value = sessionStorage.getItem('flag') === 'true'\n        }\n        \n        // 处理头像上传成功\n        const handleAvatarSuccess = (res) => {\n            if (res.code !== 200) {\n                proxy.$message.error('头像上传失败')\n                return\n            }\n            \n            proxy.$message.success('头像上传成功')\n            userInfo.url = res.data\n        }\n        \n        // 更新用户信息\n        const updateUserInfo = async () => {\n            try {\n                const userUpdateDTO = {\n                    userAvatar: userInfo.url,\n                    userName: userInfo.name,\n                    userEmail: userInfo.email\n                }\n                \n                const response = await proxy.$axios.put('/user/update', userUpdateDTO)\n                const { data } = response\n                \n                if (data.code === 200) {\n                    dialogOperaion.value = false\n                    tokenCheckLoad()\n                    \n                    proxy.$swal.fire({\n                        title: '修改成功',\n                        text: data.msg,\n                        icon: 'success',\n                        showConfirmButton: false,\n                        timer: 1000\n                    })\n                }\n            } catch (error) {\n                dialogOperaion.value = false\n                \n                proxy.$swal.fire({\n                    title: '修改失败',\n                    text: error.message || '发生未知错误',\n                    icon: 'error',\n                    showConfirmButton: false,\n                    timer: 2000\n                })\n                \n                console.error('修改个人信息异常:', error)\n            }\n        }\n        \n        // 处理头部事件\n        const eventListener = (event) => {\n            if (event === 'center') {\n                dialogOperaion.value = true\n            } else if (event === 'loginOut') {\n                loginOut()\n            }\n        }\n        \n        // 退出登录\n        const loginOut = async () => {\n            const confirmed = await proxy.$swalConfirm({\n                title: '确认退出登录?',\n                text: '退出后需重新登录',\n                icon: 'warning',\n                showCancelButton: true,\n                confirmButtonText: '确认',\n                cancelButtonText: '取消',\n                confirmButtonColor: '#ff5722',\n                cancelButtonColor: '#909399',\n                iconColor: '#ff9800',\n                customClass: {\n                    popup: 'modern-swal-popup',\n                    header: 'modern-swal-header',\n                    title: 'modern-swal-title',\n                    content: 'modern-swal-content',\n                    confirmButton: 'modern-swal-confirm-button',\n                    cancelButton: 'modern-swal-cancel-button',\n                    actions: 'modern-swal-actions'\n                },\n                buttonsStyling: true,\n                showClass: {\n                    popup: 'animate__animated animate__fadeInUp animate__faster'\n                },\n                hideClass: {\n                    popup: 'animate__animated animate__fadeOutDown animate__faster'\n                }\n            })\n            \n            if (confirmed) {\n                proxy.$swal.fire({\n                    title: '退出成功',\n                    text: '正在返回登录页面...',\n                    icon: 'success',\n                    showConfirmButton: false,\n                    timer: 1000,\n                    iconColor: '#67C23A',\n                    customClass: {\n                        popup: 'modern-swal-popup',\n                        title: 'modern-swal-title',\n                        content: 'modern-swal-content'\n                    }\n                })\n                \n                setTimeout(() => {\n                    clearToken()\n                    proxy.$router.push(\"/login\")\n                }, 1000)\n            }\n        }\n        \n        // 切换侧边栏\n        const selectOperation = (flagValue) => {\n            flag.value = flagValue\n            sessionStorage.setItem('flag', flagValue)\n        }\n        \n        // 选择菜单项\n        这里的path\n        const handleRouteSelect = (path) => {\n            if (proxy.$route.path === path) return\n            \n            const route = adminRoutes.value.find(item => item.path === path)\n            if (route) {\n                tag.value = route.name\n                proxy.$router.push(path)\n            }\n        }\n        \n        // 验证token并加载用户信息\n        const tokenCheckLoad = async () => {\n            try {\n                const res = await request.get('user/auth')\n                \n                // 处理错误\n                if (res.data.code === 400) {\n                    proxy.$message.error(res.data.msg)\n                    proxy.$router.push('/login')\n                    return\n                }\n                \n                // 设置用户信息\n                const { id, userAvatar: url, userName: name, userRole: role, userEmail: email } = res.data.data\n                Object.assign(userInfo, { id, url, name, role, email })\n                \n                // 验证用户角色\n                if (role !== 1) {\n                    proxy.$message.warning('您没有管理员权限，正在跳转...')\n                    proxy.$router.push('/user')\n                }\n            } catch (error) {\n                console.error('获取用户信息失败:', error)\n                proxy.$message.error('认证失败，请重新登录')\n                proxy.$router.push('/login')\n            }\n        }\n\n        // 路由监听\n        watch(() => proxy.$route, () => {\n            updatePageTitle()\n        })\n\n        // 生命周期\n        onMounted(() => {\n            // 直接使用路由配置 - 避免 Vue Router 4 的复杂性\n            const adminRoutes_static = [\n                { path: \"/adminLayout\", name: '数据总览', icon: 'DataAnalysis' },\n                { path: \"/userManage\", name: '用户管理', icon: 'User' },\n                { path: \"/bookManage\", name: '书籍管理', icon: 'Reading' },\n                { path: \"/bookOrderHistoryManage\", name: '借阅记录', icon: 'View' },\n                { path: \"/bookRssHistoryManage\", name: '图书订阅', icon: 'Connection' },\n                { path: \"/rssNotificationManage\", name: '订阅通知', icon: 'Message' },\n                { path: \"/bookshelfManage\", name: '书架管理', icon: 'Folder' },\n                { path: \"/categoryManage\", name: '类别管理', icon: 'CollectionTag' },\n                { path: \"/readerProposalManage\", name: '读者建议', icon: 'EditPen' },\n                { path: \"/noticeManage\", name: '公告管理', icon: 'Bell' },\n                { path: \"/userOperationLogManage\", name: '行为日志', icon: 'Notebook' }\n            ]\n            adminRoutes.value = adminRoutes_static\n            console.log('设置admin路由:', adminRoutes.value)\n            \n            // 初始化数据\n            tokenCheckLoad()\n            loadSidebarState()\n            \n            // 初始化标题\n            updatePageTitle()\n        })\n\n        return {\n            adminRoutes,\n            userInfo,\n            flag,\n            tag,\n            colorLogo,\n            bagMenu,\n            dialogOperaion,\n            updatePageTitle,\n            loadSidebarState,\n            handleAvatarSuccess,\n            updateUserInfo,\n            eventListener,\n            loginOut,\n            selectOperation,\n            handleRouteSelect,\n            tokenCheckLoad\n        }\n    }\n}\n</script>\n\n<style scoped lang=\"scss\">\n// 主布局\n.admin-layout {\n    display: flex;\n    height: 100vh;\n    background: linear-gradient(135deg, #e8f0ff 0%, #d6e9ff 50%, #e8e0ff 100%);\n    background-attachment: fixed;\n    overflow: hidden;\n    position: relative;\n    \n    &::before {\n        content: '';\n        position: absolute;\n        top: 0;\n        left: 0;\n        right: 0;\n        bottom: 0;\n        background: \n            radial-gradient(circle at 15% 20%, rgba(255, 130, 180, 0.15) 0%, rgba(255, 130, 180, 0) 35%),\n            radial-gradient(circle at 85% 70%, rgba(100, 180, 255, 0.2) 0%, rgba(100, 180, 255, 0) 35%);\n        pointer-events: none;\n    }\n}\n\n// 侧边栏\n.sidebar {\n    width: 240px;\n    background-color: rgba(255, 255, 255, 0.9);\n    backdrop-filter: blur(10px);\n    box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);\n    display: flex;  /* 使用 flex 布局，便于内部纵向排列 */\n    flex-direction: column;  /* 子元素垂直排列（从上到下） */\n    transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);  /* 过渡动画，all：监听所有可过渡的 CSS 属性变化 0.3s：动画持续时间 0.3 秒 开始时加速（0.4 控制）中间匀速 结束时减速（0.2, 1 控制）*/\n    z-index: 10;  /* 提高层级，确保位于主要内容之上 */\n    flex-shrink: 0;  /* 在 flex 容器中不允许收缩，保持设定宽度 */\n    border-radius: 0 16px 16px 0;  /* 右上与右下圆角，从左上角开始顺时针转动 */\n    overflow: hidden;  /* 隐藏溢出内容，配合圆角避免内容溢出显示 */\n    margin-right: 0px;  /* 右侧外边距为0 */\n    position: relative;  /* 相对定位以便内部绝对定位元素参照该容器 */\n}\n\n.sidebar-collapsed {\n    width: 90px;\n}\n\n.logo-container {\n    height: 70px;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    border-bottom: 1px solid rgba(0, 0, 0, 0.04);\n    padding: 0 16px;\n}\n\n.menu-wrapper {\n    flex: 1;\n    overflow-y: auto;\n    padding: 8px 0;\n    \n    &::-webkit-scrollbar {\n        width: 4px;\n    }\n    \n    &::-webkit-scrollbar-track {\n        background: transparent;\n    }\n    \n    &::-webkit-scrollbar-thumb {\n        background-color: rgba(0, 0, 0, 0.1);\n        border-radius: 2px;\n        \n        &:hover {\n            background-color: rgba(0, 0, 0, 0.2);\n        }\n    }\n}\n\n// 主内容区\n.main-container {\n    flex: 1;\n    display: flex;\n    flex-direction: column;\n    overflow: hidden;\n    padding: 16px 20px 16px 32px;\n    background-color: transparent;\n    transition: all 0.3s ease;\n    margin-left: -12px;\n}\n\n.main-expanded {\n    padding-left: 24px;\n}\n\n.header {\n    height: 70px;\n    background-color: rgba(255, 255, 255, 0.9);\n    backdrop-filter: blur(10px);\n    borde