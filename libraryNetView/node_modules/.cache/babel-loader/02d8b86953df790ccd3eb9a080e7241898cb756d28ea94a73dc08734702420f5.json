nent(\"el-button\");\n  const _component_el_dialog = _resolveComponent(\"el-dialog\");\n  return _openBlock(), _createElementBlock(\"div\", _hoisted_1, [_createCommentVNode(\" 左侧导航 \"), _createElementVNode(\"div\", {\n    class: _normalizeClass([\"sidebar\", {\n      'sidebar-collapsed': _ctx.flag\n    }])\n  }, [_createElementVNode(\"div\", _hoisted_2, [_createVNode(_component_Logo, {\n    name: \"书海智控台\",\n    flag: _ctx.flag,\n    bag: _ctx.colorLogo\n  }, null, 8 /* PROPS */, [\"flag\", \"bag\"])]), _createElementVNode(\"div\", _hoisted_3, [_createCommentVNode(\" @select监听子组件handleSelect方法中emit('select', activeIndex.value)传递过来的参数 \"), _createVNode(_component_AdminMenu, {\n    flag: _ctx.flag,\n    routes: _ctx.adminRoutes,\n    bag: _ctx.bagMenu,\n    onSelect: _ctx.handleRouteSelect\n  }, null, 8 /* PROPS */, [\"flag\", \"routes\", \"bag\", \"onSelect\"])])], 2 /* CLASS */), _createCommentVNode(\" 右侧内容区 \"), _createElementVNode(\"div\", {\n    class: _normalizeClass([\"main-container\", {\n      'main-expanded': _ctx.flag\n    }])\n  }, [_createElementVNode(\"div\", _hoisted_4, [_createVNode(_component_LevelHeader, {\n    onEventListener: _ctx.eventListener,\n    onSelectOperation: _ctx.selectOperation,\n    tag: _ctx.tag,\n    userInfo: _ctx.userInfo\n  }, null, 8 /* PROPS */, [\"onEventListener\", \"onSelectOperation\", \"tag\", \"userInfo\"])]), _createElementVNode(\"div\", _hoisted_5, [_createVNode(_component_router_view)])], 2 /* CLASS */), _createCommentVNode(\" 个人中心弹窗 \"), _createVNode(_component_el_dialog, {\n    modelValue: _ctx.dialogOperaion,\n    \"onUpdate:modelValue\": _cache[3] || (_cache[3] = $event => _ctx.dialogOperaion = $event),\n    width: \"380px\",\n    \"custom-class\": \"user-center-dialog\",\n    \"show-close\": true\n  }, {\n    default: _withCtx(() => [_createElementVNode(\"div\", _hoisted_6, [_cache[9] || (_cache[9] = _createElementVNode(\"div\", {\n      class: \"user-center-header\"\n    }, [_createElementVNode(\"h3\", null, \"个人中心\")], -1 /* CACHED */)), _createElementVNode(\"div\", _hoisted_7, [_createElementVNode(\"div\", _hoisted_8, [_createElementVNode(\"label\", _hoisted_9, [_createVNode(_component_el_icon, null, {\n      default: _withCtx(() => [_createVNode(_component_UserFilled)]),\n      _: 1 /* STABLE */\n    }), _cache[4] || (_cache[4] = _createElementVNode(\"span\", null, \"头像\", -1 /* CACHED */))]), _createElementVNode(\"div\", _hoisted_10, [_createVNode(_component_el_upload, {\n      class: \"avatar-uploader\",\n      action: \"/api/library-system-api/v1.0/file/upload\",\n      \"show-file-list\": false,\n      \"on-success\": _ctx.handleAvatarSuccess\n    }, {\n      default: _withCtx(() => [_ctx.userInfo.url ? (_openBlock(), _createElementBlock(\"img\", {\n        key: 0,\n        src: _ctx.userInfo.url,\n        class: \"user-avatar\"\n      }, null, 8 /* PROPS */, _hoisted_11)) : (_openBlock(), _createBlock(_component_el_icon, {\n        key: 1,\n        class: \"avatar-uploader-icon\"\n      }, {\n        default: _withCtx(() => [_createVNode(_component_Plus)]),\n        _: 1 /* STABLE */\n      }))]),\n      _: 1 /* STABLE */\n    }, 8 /* PROPS */, [\"on-success\"])])]), _createElementVNode(\"div\", _hoisted_12, [_createElementVNode(\"label\", _hoisted_13, [_createVNode(_component_el_icon, null, {\n      default: _withCtx(() => [_createVNode(_component_User)]),\n      _: 1 /* STABLE */\n    }), _cache[5] || (_cache[5] = _createElementVNode(\"span\", null, \"用户名\", -1 /* CACHED */))]), _createVNode(_component_el_input, {\n      modelValue: _ctx.userInfo.name,\n      \"onUpdate:modelValue\": _cache[0] || (_cache[0] = $event => _ctx.userInfo.name = $event),\n      placeholder: \"请输入用户名\"\n    }, null, 8 /* PROPS */, [\"modelValue\"])]), _createElementVNode(\"div\", _hoisted_14, [_createElementVNode(\"label\", _hoisted_15, [_createVNode(_component_el_icon, null, {\n      default: _withCtx(() => [_createVNode(_component_Message)]),\n      _: 1 /* STABLE */\n    }), _cache[6] || (_cache[6] = _createElementVNode(\"span\", null, \"邮箱\", -1 /* CACHED */))]), _createVNode(_component_el_input, {\n      modelValue: _ctx.userInfo.email,\n      \"onUpdate:modelValue\": _cache[1] || (_cache[1] = $event => _ctx.userInfo.email = $event),\n      placeholder: \"请输入邮箱\"\n    }, null, 8 /* PROPS */, [\"modelValue\"])])]), _createElementVNode(\"div\", _hoisted_16, [_createVNode(_component_el_button, {\n      onClick: _cache[2] || (_cache[2] = $event => _ctx.dialogOperaion = false)\n    }, {\n      default: _withCtx(() => _cache[7] || (_cache[7] = [_createTextVNode(\"取 消\", -1 /* CACHED */)])),\n      _: 1 /* STABLE */,\n      __: [7]\n    }), _createVNode(_component_el_button, {\n      type: \"primary\",\n      onClick: _ctx.updateUserInfo\n    }, {\n      default: _withCtx(() => _cache[8] || (_cache[8] = [_createTextVNode(\"确认修改\", -1 /* CACHED */)])),\n      _: 1 /* STABLE */,\n      __: [8]\n    }, 8 /* PROPS */, [\"onClick\"])])])]),\n    _: 1 /* STABLE */\n  }, 8 /* PROPS */, [\"modelValue\"])]);\n}","map":{"version":3,"names":["class","_createElementBlock","_hoisted_1","_createCommentVNode","_createElementVNode","_normalizeClass","_ctx","flag","_hoisted_2","_createVNode","_component_Logo","name","bag","colorLogo","_hoisted_3","_component_AdminMenu","routes","adminRoutes","bagMenu","onSelect","handleRouteSelect","_hoisted_4","_component_LevelHeader","onEventListener","eventListener","onSelectOperation","selectOperation","tag","userInfo","_hoisted_5","_component_router_view","_component_el_dialog","dialogOperaion","$event","width","_hoisted_6","_hoisted_7","_hoisted_8","_hoisted_9","_component_el_icon","_component_UserFilled","_hoisted_10","_component_el_upload","action","handleAvatarSuccess","url","src","_createBlock","_component_Plus","_hoisted_12","_hoisted_13","_component_User","_component_el_input","placeholder","_hoisted_14","_hoisted_15","_component_Message","email","_hoisted_16","_component_el_button","onClick","_cache","type","updateUserInfo"],"sources":["E:\\code\\IntelliJ_IDEAProjects\\LibrarySystem\\libraryNetView\\src\\views\\admin\\Home.vue"],"sourcesContent":["<template>\n    <div class=\"admin-layout\">\n        <!-- 左侧导航 -->\n        <div class=\"sidebar\" :class=\"{'sidebar-collapsed': flag}\">\n            <div class=\"logo-container\">\n                <Logo name=\"书海智控台\" :flag=\"flag\" :bag=\"colorLogo\" />\n            </div>\n            <div class=\"menu-wrapper\">\n                <!-- @select监听子组件handleSelect方法中emit('select', activeIndex.value)传递过来的参数 -->\n                <AdminMenu :flag=\"flag\" :routes=\"adminRoutes\" :bag=\"bagMenu\" @select=\"handleRouteSelect\" />\n            </div>\n        </div>\n\n        <!-- 右侧内容区 -->\n        <div class=\"main-container\" :class=\"{'main-expanded': flag}\">\n            <div class=\"header\">\n                <LevelHeader \n                    @eventListener=\"eventListener\" \n                    @selectOperation=\"selectOperation\" \n                    :tag=\"tag\"\n                    :userInfo=\"userInfo\" \n                />\n            </div>\n            <div class=\"content\">\n                <router-view></router-view>\n            </div>\n        </div>\n\n        <!-- 个人中心弹窗 -->\n        <el-dialog \n            v-model=\"dialogOperaion\" \n            width=\"380px\" \n            custom-class=\"user-center-dialog\"\n            :show-close=\"true\">\n            <div class=\"dialog-content-wrapper\">\n            <div class=\"user-center-header\">\n                <h3>个人中心</h3>\n            </div>\n            <div class=\"user-center-body\">\n                <div class=\"form-item\">\n                    <label class=\"form-label\">\n                        <el-icon><UserFilled /></el-icon>\n                        <span>头像</span>\n                    </label>\n                    <div class=\"avatar-wrapper\">\n                        <el-upload \n                            class=\"avatar-uploader\" \n                            action=\"/api/library-system-api/v1.0/file/upload\"\n                            :show-file-list=\"false\" \n                            :on-success=\"handleAvatarSuccess\">\n                            <img v-if=\"userInfo.url\" :src=\"userInfo.url\" class=\"user-avatar\">\n                            <el-icon v-else class=\"avatar-uploader-icon\"><Plus /></el-icon>\n                        </el-upload>\n                    </div>\n                </div>\n                <div class=\"form-item\">\n                    <label class=\"form-label\">\n                        <el-icon><User /></el-icon>\n                        <span>用户名</span>\n                    </label>\n                    <el-input v-model=\"userInfo.name\" placeholder=\"请输入用户名\"></el-input>\n                </div>\n                <div class=\"form-item\">\n                    <label class=\"form-label\">\n                        <el-icon><Message /></el-icon>\n                        <span>邮箱</span>\n                    </label>\n                    <el-input v-model=\"userInfo.email\" placeholder=\"请输入邮箱\"></el-input>\n                </div>\n            </div>\n                <div class=\"user-center-footer\">\n                <el-button @click=\"dialogOperaion = false\">取 消</el-button>\n                <el-button type=\"primary\" @click=\"updateUserInfo\">确认修改</el-button>\n                </div>\n            </div>\n        </el-dialog>\n    </div>\n</template>\n\n<script>\nimport { ref, reactive, onMounted, watch, getCurrentInstance } from 'vue'\nimport request from \"@/utils/request.js\"\nimport { clearToken } from \"@/utils/storage\"\nimport AdminMenu from '@/components/VerticalMenu.vue'\nimport Logo from '@/components/Logo.vue'\nimport LevelHeader from '@/components/LevelHeader.vue'\nimport { UserFilled, Plus, User, Message } from '@element-plus/icons-vue'\n\nexport default {\n    name: \"AdminHome\",\n    components: {\n        Logo,\n        LevelHeader,\n        AdminMenu,\n        UserFilled,\n        Plus,\n        User,\n        Message\n    },\n    setup() {\n        const { proxy } = getCurrentInstance()\n        \n        // 响应式数据\n        const adminRoutes = ref([])\n        const userInfo = reactive({\n            id: null,\n            url: '',\n            name: '',\n            role: null,\n            email: ''\n        })\n        const flag = ref(false)\n        const tag = ref('仪表盘')\n        const colorLogo = ref('#333')\n        const bagMenu = ref('transparent')\n        const dialogOperaion = ref(false)\n\n        // 方法定义\n        // 更新页面标题\n        const updatePageTitle = () => {\n            const currentPath = proxy.$route.path\n            const currentRoute = adminRoutes.value.find(route => route.path === currentPath)\n            if (currentRoute) {\n                tag.value = currentRoute.name\n            }\n        }\n        \n        // 加载侧边栏状态\n        const loadSidebarState = () => {\n            flag.value = sessionStorage.getItem('flag') === 'true'\n        }\n        \n        // 处理头像上传成功\n        const handleAvatarSuccess = (res) => {\n            if (res.code !== 200) {\n                proxy.$message.error('头像上传失败')\n                return\n            }\n            \n            proxy.$message.success('头像上传成功')\n            userInfo.url = res.data\n        }\n        \n        // 更新用户信息\n        const updateUserInfo = async () => {\n            try {\n                const userUpdateDTO = {\n                    userAvatar: userInfo.url,\n                    userName: userInfo.name,\n                    userEmail: userInfo.email\n                }\n                \n                const response = await proxy.$axios.put('/user/update', userUpdateDTO)\n                const { data } = response\n                \n                if (data.code === 200) {\n                    dialogOperaion.value = false\n                    tokenCheckLoad()\n                    \n                    proxy.$swal.fire({\n                        title: '修改成功',\n                        text: data.msg,\n                        icon: 'success',\n                        showConfirmButton: false,\n                        timer: 1000\n                    })\n                }\n            } catch (error) {\n                dialogOperaion.value = false\n                \n                proxy.$swal.fire({\n                    title: '修改失败',\n                    text: error.message || '发生未知错误',\n                    icon: 'error',\n                    showConfirmButton: false,\n                    timer: 2000\n                })\n                \n                console.error('修改个人信息异常:', error)\n            }\n        }\n        \n        // 处理头部事件\n        const eventListener = (event) => {\n            if (event === 'center') {\n                dialogOperaion.value = true\n            } else if (event === 'loginOut') {\n                loginOut()\n            }\n        }\n        \n        // 退出登录\n        const loginOut = async () => {\n            const confirmed = await proxy.$swalConfirm({\n                title: '确认退出登录?',\n                text: '退出后需重新登录',\n                icon: 'warning',\n                showCancelButton: true,\n                confirmButtonText: '确认',\n                cancelButtonText: '取消',\n                confirmButtonColor: '#ff5722',\n                cancelButtonColor: '#909399',\n                iconColor: '#ff9800',\n                customClass: {\n                    popup: 'modern-swal-popup',\n                    header: 'modern-swal-header',\n                    title: 'modern-swal-title',\n                    content: 'modern-swal-content',\n                    confirmButton: 'modern-swal-confirm-button',\n                    cancelButton: 'modern-swal-cancel-button',\n                    actions: 'modern-swal-actions'\n                },\n                buttonsStyling: true,\n                showClass: {\n                    popup: 'animate__animated animate__fadeInUp animate__faster'\n                },\n                hideClass: {\n                    popup: 'animate__animated animate__fadeOutDown animate__faster'\n                }\n            })\n            \n            if (confirmed) {\n                proxy.$swal.fire({\n                    title: '退出成功',\n                    text: '正在返回登录页面...',\n                    icon: 'success',\n                    showConfirmButton: false,\n                    timer: 1000,\n                    iconColor: '#67C23A',\n                    customClass: {\n                        popup: 'modern-swal-popup',\n                        title: 'modern-swal-title',\n                        content: 'modern-swal-content'\n                    }\n                })\n                \n                setTimeout(() => {\n                    clearToken()\n                    proxy.$router.push(\"/login\")\n                }, 1000)\n            }\n        }\n        \n        // 切换侧边栏\n        const selectOperation = (flagValue) => {\n            flag.value = flagValue\n            sessionStorage.setItem('flag', flagValue)\n        }\n        \n        // 选择菜单项\n        // 这里的path参数内容是子组件传递的emit('select', activeIndex.value)\n        const handleRouteSelect = (path) => {\n            if (proxy.$route.path === path) return\n            \n            const route = adminRoutes.value.find(item => item.path === path)\n            if (route) {\n                tag.value = route.name\n                proxy.$router.push(path)\n            }\n        }\n        \n        // 验证token并加载用户信息\n        const tokenCheckLoad = async () => {\n            try {\n                const res = await request.get('user/auth')\n                \n                // 处理错误\n                if (res.data.code === 400) {\n                    proxy.$message.error(res.data.msg)\n                    proxy.$router.push('/login')\n                    return\n                }\n                \n                // 设置用户信息\n                const { id, userAvatar: url, userName: name, userRole: role, userEmail: email } = res.data.data\n                批量更新响应式对象 userInfo 的属性值，等价于userInfo.id = id、userInfo.url = url、userInfo.name = name、userInfo.role = role、userInfo.email = email\n                Object.assign(userInfo, { id, url, name, role, email })\n                \n                // 验证用户角色\n                if (role !== 1) {\n                    proxy.$message.warning('您没有管理员权限，正在跳转...')\n                    proxy.$router.push('/user')\n                }\n            } catch (error) {\n                console.error('获取用户信息失败:', error)\n                proxy.$message.error('认证失败，请重新登录')\n                proxy.$router.push('/login')\n            }\n        }\n\n        // 路由监听\n        watch(() => proxy.$route, () => {\n            updatePageTitle()\n        })\n\n        // 生命周期\n        onMounted(() => {\n            // 直接使用路由配置 - 避免 Vue Router 4 的复杂性\n            const adminRoutes_static = [\n                { path: \"/adminLayout\", name: '数据总览', icon: 'DataAnalysis' },\n                { path: \"/userManage\", name: '用户管理', icon: 'User' },\n                { path: \"/bookManage\", name: '书籍管理', icon: 'Reading' },\n                { path: \"/bookOrderHistoryManage\", name: '借阅记录', icon: 'View' },\n                { path: \"/bookRssHistoryManage\", name: '图书订阅', icon: 'Connection' },\n                { path: \"/rssNotificationManage\", name: '订阅通知', icon: 'Message' },\n                { path: \"/bookshelfManage\", name: '书架管理', icon: 'Folder' },\n                { path: \"/categoryManage\", name: '类别管理', icon: 'CollectionTag' },\n                { path: \"/readerProposalManage\", name: '读者建议', icon: 'EditPen' },\n                { path: \"/noticeManage\", name: '公告管理', icon: 'Bell' },\n                { path: \"/userOperationLogManage\", name: '行为日志', icon: 'Notebook' }\n            ]\n            adminRoutes.value = adminRoutes_static\n            console.log('设置admin路由:', adminRoutes.value)\n            \n            // 初始化数据\n            tokenCheckLoad()\n            loadSidebarState()\n            \n            // 初始化标题\n            updatePageTitle()\n        })\n\n        return {\n            adminRoutes,\n            userInfo,\n            flag,\n            tag,\n            colorLogo,\n            bagMenu,\n            dialogOperaion,\n            updatePageTitle,\n            loadSidebarState,\n            handleAvatarSuccess,\n            updateUserInfo,\n            eventListener,\n            loginOut,\n            selectOperation,\n            handleRouteSelect,\n            tokenCheckLoad\n        }\n    }\n}\n</script>\n\n<style scoped lang=\"scss\">\n// 主布局\n.admin-layout {\n    display: flex;\n    height: 100vh;\n    background: linear-gradient(135deg, #e8f0ff 0%, #d6e9ff 50%, #e8e0ff 100%);\n    background-attachment: fixed;\n    overflow: hidden;\n    position: relative;\n    \n    &::before {\n        content: '';\n        position: absolute;\n        top: 0;\n        left: 0;\n        right: 0;\n        bottom: 0;\n        background: \n            radial-gradient(circle at 15% 20%, rgba(255, 130, 180, 0.15) 0%, rgba(255, 130, 180, 0) 35%),\n            radial-gradient(circle at 85% 70%, rgba(100, 180, 255, 0.2) 0%, rgba(100, 180, 255, 0) 35%);\n        pointer-events: none;\n    }\n}\n\n// 侧边栏\n.sidebar {\n    width: 240px;\n    background-color: rgba(255, 255, 255, 0.9);\n    backdrop-filter: blur(10px);\n    box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);\n    display: flex;  /* 使用 flex 布局，便于内部纵向排列 */\n    flex-direction: column;  /* 子元素垂直排列（从上到下） */\n    transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);  /* 过渡动画，all：监听所有可过渡的 CSS 属性变化 0.3s：动画持续时间 0.3 秒 开始时加速（0.4 控制）中间匀速 结束时减速（0.2, 1 控制）*/\n    z-index: 10;  /* 提高层级，确保位于主要内容之上 */\n    flex-shrink: 0;  /* 在 flex 容器中不允许收缩，保持设定宽度 */\n    border-radius: 0 16px 16px 0;  /* 右上与右下圆角，从左上角开始顺时针转动 */\n    overflow: hidden;  /* 隐藏溢出内容，配合圆角避免内容溢出显示 */\n    margin-right: 0px;  /* 右侧外边距为0 */\n    position: relative;  /* 相对定位以便内部绝对定位元素参照该容器 */\n}\n\n.sidebar-collapsed {\n    width: 90px;\n}\n\n.logo-container {\n    height: 70px;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    border-bottom: 1px solid rgba(0, 0, 0, 0.04);\n    padding: 0 16px;\n}\n\n.menu-wrapper {\n    flex: 1;\n    overflow-y: auto;\n    padding: 8px 0;\n    \n    &::-webkit-scrollbar {\n        width: 4px;\n    }\n    \n    &::-webkit-scrollbar-track {\n        background: transparent;\n    }\n    \n    &::-webkit-scrollbar-thumb {\n        background-color: rgba(0, 0, 0, 0.1);\n        border-radius: 2px;\n        \n        &:hover {\n            background-color: rgba(0, 0, 0, 0.2);\n        }\n    }\n}\n\n// 主内容区\n.main-container {\n    flex: 1;\n    display: flex;\n    flex-direction: column;\n    overflow: hidden;\n    padding: 16px 20px 16px 32px;\n    background-color: transparent;\n    transition: all 0.3s ease;\n    margin-left: -12px;\n}\n\n.main-expanded {\n    padding-left: 24px;\n}\n\n.header {\n    height: 70px;\n    background-color: rgba(255, 255, 255, 0.9);\n    backdrop-filter: blur(10px);\n    border-radius: 16px;\n    margin-bottom: 16px;\n    box-shadow: 0 4px 15px rgba(125, 150, 255, 0.1);\n    display: flex;\n    align-items: center;\n}\n\n.content {\n    flex: 1;\n    background-color: rgba(255, 255, 255, 0.9);\n    backdrop-filter: blur(10px);\n    border-radius: 16px;\n    padding: 24px;\n    overflow-y: auto;\n    box-shadow: 0 4px 15px rgba(125, 150, 255, 0.1);\n    \n    &::-webkit-scrollbar {\n        width: 6px;\n    }\n    \n    &::-webkit-scrollbar-track {\n        background: rgba(240, 242, 245, 0.5);\n        border-radius: 3px;\n    }\n    \n    &::-webkit-scrollbar-thumb {\n        background-color: rgba(0, 0, 0, 0.1);\n        border-radius: 3px;\n        \n        &:hover {\n            background-color: rgba(0, 0, 0, 0.2);\n        }\n    }\n}\n\n// 用户中心弹窗\n.user-center-dialog {\n    border-ra